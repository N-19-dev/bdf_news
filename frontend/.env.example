# Firebase Configuration for Community Voting
# Get these values from Firebase Console → Project Settings → General → Your apps → SDK setup and configuration

# Firebase API Key
VITE_FIREBASE_API_KEY=AIzaSyXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX

# Firebase Auth Domain (your-project.firebaseapp.com)
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com

# Firebase Project ID
VITE_FIREBASE_PROJECT_ID=your-project-id

# Firebase Storage Bucket (your-project.appspot.com)
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com

# Firebase Messaging Sender ID
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789

# Firebase App ID
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef
